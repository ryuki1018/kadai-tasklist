
<!--サインインページ-->

<h1>サインインページ</h1>
<%= form_with(model: @user, local: true) do |f| %>

<%= f.label :name, 'Name' %>

<%= f.text_field :name %>

<%= f.label :email, 'email' %>

<%= f.text_field :email %>
<%= f.label :password, 'password' %>

<%= f.text_field :password %>

<%= f.label :password_confirmation, 'confirmation' %>

<%= f.text_field :password_confirmation %>

<%= f.submit 'signup'%>

<% end %>